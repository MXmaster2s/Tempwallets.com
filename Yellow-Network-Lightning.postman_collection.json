            "body": {
              "mode": "raw",
              "raw": "{\n  \"userId\": \"{{userId}}\",\n  \"chain\": \"{{chain}}\",\n  \"asset\": \"{{asset}}\",\n  \"amount\": \"0.05\"\n}"
